SetInfo(iLine, sText)
{
	self SetClientDvar("ui_hudDevI" + iLine, sText);
}

CIPrintLnLines(sText)
{
	sLines = StrTok(sText, "\n");
	foreach (sLine in sLines)
		self IPrintLn(sLine);
}