;	player endon( "death" );		activator = GetActivator();		if( !isdefined( activator ) )		return;			origin1 = getent( "knif_1", "targetname" );	origin2 = getent( "knif_0", "targetname" );		if( !i )	{		origin1 PlaceSpawnPoint();		origin2 PlaceSpawnPoint();	}		activator FreezeControls( true );	activator SetOrigin( origin1.origin );	activator SetPlayerAngles( origin1.angles );	activator TakeAllWeapons();	activator GiveWeapon( "knife_mp" );	activator.health = 100;		player FreezeControls( true );	player SetOrigin( origin2.origin );	player SetPlayerAngles( origin2.angles );	player TakeAllWeapons();	player GiveWeapon( "knife_mp" );	player.health = 100;		ambientPlay("music_knife");		for(i = 0;i < 5;i++)		iprintlnbold( " " );		iprintlnbold( "^1=== ^7Knife Fight ^1===" );	iprintlnbold( "^1"+ activator.name +"^7 vs ^1"+player.name );		wait 3;		activator FreezeControls( false );	activator SwitchToWeapon( "knife_mp" );		player FreezeControls( false );	player SwitchToWeapon( "knife_mp" );		activator waittill( "death" );}GetActivator(){	players = getentarray( "player", "classname" );		for(i = 0;i < players.size;i++)	{		player = players[i];				if( isdefined( player ) && isplayer( player ) && isalive( player ) && player.pers["team"] == "axis"	)			return player;	}		return undefined;}